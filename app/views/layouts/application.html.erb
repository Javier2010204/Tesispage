    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/fullPage.js/2.6.6/jquery.fullPage.css" integrity="sha512-TiOnKhAn9+AkO6g9Racg1r+GEF6d2nS/0Ye8LPYVSRDQ40TDQ9EO1ZzIYaiSpnh2hzMKtGZkVSKqrjR692SuBg==" crossorigin="anonymous" />
